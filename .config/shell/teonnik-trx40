# vim: filetype=sh

export TEONNIK_PERSONAL="${HOME}/personal"

export SSH_KEY_PATH="~/.ssh/id_ed25519"

# OMNI VARS
export OMNI_USER=svc-drivesim-unit-tests
export OMNI_PASS="$(lpass show --password omni-env)"

# DRIVE Sim prerequisite for building
export NVM_GTLAPI_TOKEN="$(lpass show --notes NVM_GTLAPI_TOKEN)"

# Path to `nsys` and `ncu`
export PATH=${HOME}/install/ncu:$PATH
export PATH=${HOME}/install/nsys/opt/nvidia/nsight-systems/2023.1.1/bin:$PATH

source ~/.config/shell/i3env
